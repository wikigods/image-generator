{
  "name": "wikigods/image-generator",
  "description": "Dynamic image generator for Laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "Jc So",
      "email": "wikigods@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "ext-gd": "*",
    "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
    "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "WikiGods\\ImageGenerator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WikiGods\\ImageGenerator\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WikiGods\\ImageGenerator\\ImageGeneratorServiceProvider"
      ],
      "aliases": {
        "ImageGenerator": "WikiGods\\ImageGenerator\\Facades\\ImageGenerator"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}